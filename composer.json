{
    "name": "connorhu/nav-online-invoice",
    "description": "PHP Library for NAV Online Invoice API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "NAV\\OnlineInvoice\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NAV\\OnlineInvoice\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.3",
        "ext-zlib": "*",
        "ext-openssl": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "doctrine/collections": "^1.7",
        "symfony/deprecation-contracts": "^3.4",
        "symfony/intl": "^6.4 | ^7.2"
    },
    "require-dev": {
        "symfony/serializer": "^6.4 | ^7.2",
        "symfony/validator": "^6.4 | ^7.2",
        "symfony/var-dumper": "^6.4 | ^7.2",
        "symfony/http-client": "^6.4 | ^7.2",
        "symfony/config": "^6.4 | ^7.2"
    },
    "config": {
        "sort-packages": true
    }
}
