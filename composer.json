{
    "name": "connorhu/nav-online-invoice",
    "description": "PHP Library for NAV Online Invoice API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "NAV\\OnlineInvoice\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NAV\\OnlineInvoice\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-zlib": "*",
        "doctrine/collections": "^1.7",
        "symfony/intl": "^6.4"
    },
    "require-dev": {
        "symfony/serializer": "^5.4|^6.0",
        "symfony/validator": "^5.4|^6.0",
        "symfony/var-dumper": "^5.4|^6.0",
        "symfony/http-client": "^5.4|^6.0",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/config": "^5.4|^6.0"
    },
    "config": {
        "sort-packages": true
    }
}
